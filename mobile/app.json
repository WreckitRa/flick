{
  "expo": {
    "name": "Flick",
    "slug": "flick",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFD93D"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.flick.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#06B6D4"
      },
      "package": "com.flick.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "flick",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "81d97615-87de-443a-8173-c80a161146ec"
      },
      "apiUrl": "https://flick-backend-production.up.railway.app/"
    }
  }
}
